<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_coffee_product" model="ir.sequence">
            <field name="name">Product sequence</field>
            <field name="code">coffee.product.sequence</field>
            <field name="prefix">CP</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_coffee_employee" model="ir.sequence">
            <field name="name">Employee sequence</field>
            <field name="code">coffee.employee.sequence</field>
            <field name="prefix">CE</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>